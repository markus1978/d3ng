{
  "name": "d3ng",
  "version": "0.1.2",
  "description": "Interactive data visualizations with Angular 2 and d3.js.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markus1978/d3ng.git"
  },
  "author": "Markus Scheidgen <markus.scheidgen@gmail.com>",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:lib": "rimraf dist/lib && ngc -p src/tsconfig.lib.json && gulp && rollup -c src/tools/rollup.config.js dist/lib/d3ng.module.js > dist/lib/d3ng.bundle.js && cp package.json README.md dist/lib",
    "build:app": "ng build --prod --aot && cp dist/app/index.html dist/app/404.html && compodoc -p src/tsconfig.compodoc.json -d dist/app/documentation/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gen:highlights": "node src/tools/highlight.js src/app/stories src/assets/story-sources/ && node src/tools/highlight.js src/app/demos src/assets/demo-sources/",
    "gen:owid": "ts-node src/tools/OWIDScraper-new.ts",
    "deploy:app": "node src/tools/gh-pages.js"
  },
  "main": "d3ng.bundle.js",
  "module": "d3ng.module.js",
  "jsnext:main": "d3ng.module.js",
  "types": "d3ng.module.d.ts",
  "peerDependencies": {
    "@angular/core": ">=4.0.0"
  },
  "dependencies": {
    "@angular/animations": "^4.2.5",
    "@angular/common": "^4.2.5",
    "@angular/core": "^4.2.5",
    "@angular/forms": "^4.2.5",
    "@angular/http": "^4.2.5",
    "@angular/material": "^2.0.0-beta.7",
    "@angular/platform-browser": "^4.2.5",
    "@angular/platform-browser-dynamic": "^4.2.5",
    "@angular/router": "^4.2.5",
    "angular2-grid": "^2.0.6",
    "angular2-markdown": "^1.6.0",
    "core-js": "^2.4.1",
    "d3": "^3.5.17",
    "datamaps": "^0.5.8",
    "hammerjs": "^2.0.8",
    "joiner": "^2.1.2",
    "jsonata": "^1.2.6",
    "rxjs": "^5.4.1",
    "simple-get": "^2.6.0",
    "stream-to-string": "^1.1.0",
    "webpack": "^2.6.1",
    "x-ray": "^2.3.2",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.2.0",
    "@angular/compiler": "^4.2.5",
    "@angular/compiler-cli": "^4.2.5",
    "@compodoc/compodoc": "^1.0.0-beta.10",
    "@types/jasmine": "^2.5.53",
    "@types/node": "^6.0.79",
    "JSONStream": "^1.3.1",
    "async": "^2.5.0",
    "codelyzer": "^2.1.1",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "highlight.js": "^9.12.0",
    "jasmine-core": "^2.6.4",
    "jasmine-spec-reporter": "^3.3.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.3",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-status": "^1.0.0",
    "path": "^0.12.7",
    "protractor": "^5.1.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.43.0",
    "ts-node": "^2.1.2",
    "tslint": "^4.5.1",
    "typescript": "^2.3.4"
  }
}
