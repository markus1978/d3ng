{
  "name": "d3ng",
  "version": "0.1.2",
  "description": "Interactive data visualizations with Angular 2 and d3.js.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markus1978/d3ng.git"
  },
  "author": "Markus Scheidgen <markus.scheidgen@gmail.com>",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:lib": "rimraf dist/lib && ngc -p src/tsconfig.lib.json && gulp && rollup -c src/tools/rollup.config.js dist/lib/d3ng.module.js > dist/lib/d3ng.bundle.js && cp package.json README.md dist/lib",
    "build:app": "ng build --prod --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.compodoc.json",
    "gen:highlights": "node src/tools/highlight.js src/app/stories src/assets/story-sources/ && node src/tools/highlight.js src/app/demos src/assets/demo-sources/"
  },
  "main": "d3ng.bundle.js",
  "module": "d3ng.module.js",
  "jsnext:main": "d3ng.module.js",
  "types": "d3ng.module.d.ts",
  "peerDependencies": {
    "@angular/core": ">=4.0.0"
  },
  "dependencies": {
    "@angular/animations": "^4.2.2",
    "@angular/common": "^4.2.2",
    "@angular/core": "^4.2.2",
    "@angular/forms": "^4.2.2",
    "@angular/http": "^4.2.2",
    "@angular/material": "^2.0.0-beta.6",
    "@angular/platform-browser": "^4.2.2",
    "@angular/platform-browser-dynamic": "^4.2.2",
    "@angular/router": "^4.2.2",
    "angular2-grid": "^2.0.3",
    "core-js": "^2.4.1",
    "d3": "^3.5.17",
    "datamaps": "^0.5.8",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.4.1",
    "simple-get": "^2.6.0",
    "stream-to-string": "^1.1.0",
    "webpack": "^2.6.1",
    "x-ray": "^2.3.2",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.1",
    "@angular/compiler": "^4.2.2",
    "@angular/compiler-cli": "^4.2.2",
    "@compodoc/compodoc": "^1.0.0-beta.9",
    "@types/jasmine": "^2.5.52",
    "@types/node": "^6.0.78",
    "codelyzer": "^2.1.1",
    "gulp": "^3.9.1",
    "highlight.js": "^9.12.0",
    "jasmine-core": "^2.6.3",
    "jasmine-spec-reporter": "^3.3.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.3",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "path": "^0.12.7",
    "protractor": "^5.1.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.43.0",
    "ts-node": "^2.1.2",
    "tslint": "^4.5.1",
    "typescript": "^2.3.4"
  }
}
